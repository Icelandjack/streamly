main_skd1
  = build
      (\ @ a_djRg c_djRh n_djRi ->
         c_djRh
           (let {
              $d(%,,%)_ajJp
                = ($fMonadIOIO, $fMonadBaseControlIOIO, $fMonadThrowIO) } in
            Benchmark
              (build (\ @ b_ak2C -> unpackFoldrCString# "product"#))
              (letrec {
                 go_skkA
                   = \ n_ajPV eta_B1 ->
                       case leInt64 n_ajPV (I64# 0#) of {
                         False ->
                           case case $fRandomInt3 (I# 1#, I# 1#) eta_B1 of
                                { (# ipv_ajSV, ipv1_ajSW #) ->
                                ((foldlM'
                                    $fMonadIO
                                    ((\ b1_ajYk a1_ajYl s_ajUr ->
                                        (# s_ajUr, $fNumInt_$c* b1_ajYk a1_ajYl #))
                                     `cast` <Co:7>)
                                    (I# 1#)
                                    (fromStreamK
                                       $fMonadIO
                                       ((unfoldrMSerial
                                           $d(%,,%)_ajJp
                                           ((\ cnt_adtX eta_X9 ->
                                               case gtInt
                                                      cnt_adtX
                                                      (case ipv1_ajSW of { I# x_ajSq ->
                                                       I# (+# x_ajSq 100000#)
                                                       })
                                               of {
                                                 False ->
                                                   (# eta_X9,
                                                      Just
